{
    "name": "BruteForce",
    "displayName": "BruteForce",
    "publisher": "Orkestra",
    "description": "A autogenerated Orkestra VSCode extension for BruteForce.",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.89.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [],
    "main": "./extension.js",
    "contributes": {
        "commands": [{
            "command": "bruteforce.helloWorld",
            "title": "Hello World"
        }],
		"languages": [
		{
		    "id": "bruteforce",
		    "aliases": [ "bruteforce" ],
		    "extensions": [ ".bf" ],
		    "configuration": "./bruteforce-configuration.json"
		}
	],
		"grammars": [
		{
		    "language": "bruteforce",
		    "scopeName": "source.bf",
		    "path" : "./bruteforce.tmLanguage.json"
		}
	],
		"snippets": [
		{
		    "language": "bruteforce",
		    "path": "./bruteforce-snippets.json"
		}
	]
    },                
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.89.0",
        "@types/mocha": "^10.0.6",
        "@types/node": "18.x",
        "eslint": "^8.57.0",
        "typescript": "^5.4.5",
        "@vscode/test-cli": "^0.0.9",
        "@vscode/test-electron": "^2.4.0"
    }
}